#limpiando base
base2 <- na.omit(data.frame(base$t_per,base$t_apo1,as.numeric( base$sex),as.numeric( base$eda1),as.numeric( base$par1),
                            as.numeric( base$tam_hog),as.numeric( base$ocu_jefe)))
names(base2)
modelo.logit<-glm(base2$base.t_per~ ., data = base2,family="gaussian")
summary(modelo.logit)
